datasource db {
  provider = "mysql"
  url      = env("DEV_MYSQL_DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id              String        @default(uuid()) @id
  name            String        @db.VarChar(255)
  photos          Json          @default("[]")
}
